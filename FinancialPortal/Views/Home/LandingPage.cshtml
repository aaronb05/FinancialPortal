                                    <ul>
                                        <li>User friendly dashboard</li>
                                        <li>Start be creating your House</li>
                                    </ul>
                                </div>
                                <div class="card-footer">
                                    <a href="@Url.Action("Register","Home")" class="btn btn-round btn-neutral btn-warning">Create Account</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6">
                            <div class="card card-pricing card-plain">
                                
                                <div class="card-body">
                                    <div class="card-icon icon-warning ">
                                        <i class="now-ui-icons business_money-coins"></i>
                                    </div>
                                    <h3 class="card-title">Login</h3>
                                    <ul>
                                        <li>Check on your money</li>
                                        <li>Add other people to your house</li>
                                    </ul>
                                </div>
                                <div class="card-footer">
                                    <a href="@Url.Action("Login","Home")" class="btn btn-round btn-neutral btn-warning">Login</a>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
           
        </div>
