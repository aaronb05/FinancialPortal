                    <!--  Wizard container  -->
                    <div class="wizard-container">
                        <div class="card card-wizard" data-color="green" id="wizardProfile">
                            @using (Html.BeginForm("Wizard", "Home", FormMethod.Post))
                            {
                                @Html.AntiForgeryToken()
                                <!--  You can switch " data-color="primary" "with one of the next bright colors: "green", "orange", "red", "blue"  -->
                                <div class="card-header text-center" data-background-color="green">
                                    <h3 class="card-title" style=" padding-bottom:20px">
                                        Build Your Profile
                                    </h3>
                                    <div class="description">
                                        @*<h5>Create your house and budget!</h5>*@
                                        <div class="wizard-navigation">
                                            <ul class="nav nav-pills">
                                                <li class="nav-item">
                                                    <a class="nav-link active" href="#house" data-toggle="tab" role="tab" aria-controls="about" aria-selected="true">
                                                        <i class="now-ui-icons users_circle-08"></i> House
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" href="#account" data-toggle="tab" role="tab" aria-controls="account" aria-selected="false">
                                                        <i class="now-ui-icons business_bank"></i> Account
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" href="#budget" data-toggle="tab" role="tab" aria-controls="address" aria-selected="false">
                                                        <i class="now-ui-icons business_money-coins"></i> Budget
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" href="#budgetItem" data-toggle="tab" role="tab" aria-controls="address" aria-selected="false">
                                                        <i class="now-ui-icons shopping_bag-16"></i> Budget Item
                                                    </a>
                                                </li>

                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-body">
                                    <div class="tab-content">
                                        <div class="tab-pane show active" id="house">
                                            <h5 class="info-text">Start by creating your House</h5>

                                            <div class="row justify-content-center">
                                                <div class="col-lg-12">
                                                    <div class="row">
                                                        <div class="col-md-3"></div>
                                                        <div class="col-sm-6">
                                                            <fieldset>
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-12 form-group">
                                                                        @Html.EditorFor(model => model.Household.Name, new { htmlAttributes = new { @placeholder = "House Name", @class = "form-control" } })
                                                                        @Html.ValidationMessageFor(model => model.Household.Name, "", new { @class = "text-danger" })
                                                                    </div>
                                                                </div>
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-12 form-group">
                                                                        @Html.EditorFor(model => model.Household.Greeting, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Greeting" } })
                                                                        @Html.ValidationMessageFor(model => model.Household.Greeting, "", new { @class = "text-danger" })

                                                                    </div>
                                                                </div>
                                                            </fieldset>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                        </div>
                                        <div class="tab-pane fade" id="account">
                                            <h5 class="info-text"> Create Your Bank Account </h5>
                                            <div class="row justify-content-center">
                                                <div class="col-lg-12">
                                                    <fieldset>
                                                        <div class="row">
                                                            <div class="col-md-6">
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-12 form-group">

                                                                        @Html.EditorFor(model => model.BankAccount.Name, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Name of Account" } })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.Name, "", new { @class = "text-danger" })

                                                                    </div>
                                                                </div>
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-12 form-group">

                                                                        @Html.EditorFor(model => model.BankAccount.Description, new { htmlAttributes = new { @class = "form-control", @placeholder = "Description" } })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.Description, "", new { @class = "text-danger" })

                                                                    </div>
                                                                </div>
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-12 form-group">

                                                                        @Html.DropDownList("AccountType", EnumHelper.GetSelectList(typeof(FinancialPortal.Enumerations.AccountTypes)), "Select Account Type", new { @class = "form-control" })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.AccountType, "", new { @class = "text-danger" })

                                                                    </div>
                                                                </div>
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-6 form-group">

                                                                        @Html.EditorFor(model => model.BankAccount.StartingBalance, new { htmlAttributes = new { @class = "form-control", @placeholder = "Starting Balance" } })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.StartingBalance, "", new { @class = "text-danger" })

                                                                    </div>
                                                                    <div class="col-md-6 form-group">

                                                                        @Html.EditorFor(model => model.BankAccount.LowBalance, new { htmlAttributes = new { @class = "form-control", @placeholder = "Low Balance Alert" } })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.LowBalance, "", new { @class = "text-danger" })

                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-6">
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-12 form-group">

                                                                        @Html.EditorFor(model => model.BankAccount.Address1, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Address" } })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.Address1, "", new { @class = "text-danger" })

                                                                    </div>
                                                                </div>
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-12 form-group">

                                                                        @Html.EditorFor(model => model.BankAccount.Address2, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Address" } })

                                                                    </div>
                                                                </div>
                                                                <div class="input-group form-control-lg">

                                                                    <div class="col-md-6">
                                                                        @Html.EditorFor(model => model.BankAccount.City, new { htmlAttributes = new { @class = "form-control", @placeholder = "City" } })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.City, "", new { @class = "text-danger" })
                                                                    </div>
                                                                    <div class="col-md-6">
                                                                        @Html.EnumDropDownListFor(model => model.BankAccount.State, htmlAttributes: new { @class = "form-control" })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.State, "", new { @class = "text-danger" })
                                                                    </div>
                                                                </div>
                                                                <div class="input-group form-control-lg">
                                                                    <div class="col-md-6 form-group">
                                                                        @Html.EditorFor(model => model.BankAccount.ZipCode, new { htmlAttributes = new { @class = "form-control", @placeholder = "Zip Code" } })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.ZipCode, "", new { @class = "text-danger" })
                                                                    </div>
                                                                    <div class="col-md-6 form-group">
                                                                        @Html.EditorFor(model => model.BankAccount.Phone, new { htmlAttributes = new { @class = "form-control", @placeholder = "Phone Number" } })
                                                                        @Html.ValidationMessageFor(model => model.BankAccount.Phone, "", new { @class = "text-danger" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="tab-pane fade" id="budget">

                                            <div class="row justify-content-center">
                                                <div class="col-sm-12">
                                                    <h5 class="info-text"> Add A Budget Category </h5>
                                                </div>
                                            </div>
                                            <div class="row text-center">
                                                <div class="col-md-3"></div>
                                                <fieldset>
                                                    <div class="col-md-6">
                                                        <div class="form-group col-md-12">
                                                            @Html.EditorFor(model => model.Budget.Name, new { htmlAttributes = new { @class = "form-control", @placeholder = "Utilities" } })
                                                            @Html.ValidationMessageFor(model => model.Budget.Name, "", new { @class = "text-danger" })
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            @Html.EditorFor(model => model.Budget.Target, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Target Amount" } })
                                                            @Html.ValidationMessageFor(model => model.Budget.Target, "", new { @class = "text-danger" })
                                                        </div>

                                                    </div>

                                                </fieldset>
                                            </div>
                                            <div class="row justify-content-center" style="padding-top:50px">
                                                <div class="col-sm-12">
                                                    <h5 class="info-text"> You Can Add More Categories Later! </h5>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="tab-pane fade" id="budgetItem">

                                            <div class="row justify-content-center">
                                                <div class="col-sm-12">
                                                    <h5 class="info-text"> Add Your Budget Item </h5>
                                                </div>
                                            </div>
                                            <fieldset>
                                                <div class="row text-center">
                                                    <div class="col-md-3"></div>
                                                    <div class="col-md-6">
                                                        <div class="form-group col-md-12">
                                                            @Html.EditorFor(model => model.BudgetItem.Name, new { htmlAttributes = new { @class = "form-control", @placeholder = "Power" } })
                                                            @Html.ValidationMessageFor(model => model.BudgetItem.Name, "", new { @class = "text-danger" })
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            @Html.EditorFor(model => model.BudgetItem.Target, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Target Amount" } })
                                                            @Html.ValidationMessageFor(model => model.BudgetItem.Target, "", new { @class = "text-danger" })
                                                        </div>

                                                    </div>
                                                </div>
                                            </fieldset>
                                            <div class="row justify-content-center" style="padding-top:50px">
                                                <div class="col-sm-12">
                                                    <h5 class="info-text"> You Can Add More Item's Later! </h5>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="card-footer">
                                    <div class="pull-right">
                                        <input type='button' class='btn btn-next btn-fill btn-rose btn-wd' name='next' value='Next' />
                                        <input type='button' onclick="$(this).closest('form').submit();" class='btn btn-finish btn-fill btn-rose btn-wd' name='finish' value='Finish' />
                                    </div>
                                    <div class="pull-left">
                                        <input type='button' class='btn btn-previous btn-fill btn-default btn-wd' name='previous' value='Previous' />
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                            }
                        </div>
                    </div>
                    <!-- wizard container -->
                </div>
            </div>

        </div>
    </div>


